                  }
                }
              }
            }
          }
        },
        "security" : [ {
          "ApiKeyAuth" : [ ]
        } ]
      }
    }
  },
  "components" : {
    "schemas" : {
      "information" : {
        "required" : [ "fund_records", "uuid" ],
        "type" : "object",
        "properties" : {
          "uuid" : {
            "type" : "string"
          },
          "fund_records" : {
            "$ref" : "#/components/schemas/fund_records"
          }
        }
      },
      "fund_records" : {
        "required" : [ "dividend_category", "fund_code", "fund_name", "id", "price_currency" ],
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "integer",
            "format" : "uuid",
            "example" : 1
          },
          "fund_code" : {
            "type" : "string",
            "example" : "D100000001"
          },
          "fund_name" : {
            "type" : "string",
            "example" : "華南永昌永昌證券投資信託基金"
          },
          "price_currency" : {
            "type" : "string",
            "example" : "汽車駕駛人傷害險"
          },
          "dividend_category" : {
            "type" : "string",
            "example" : "月配"
          },
          "creat_date" : {
            "type" : "string",
            "format" : "YYYY-MM-DD",
            "example" : "2019-01-01"
          }
        }
      },
      "Net" : {
        "required" : [ "net_info", "uuid" ],
        "type" : "object",
        "properties" : {
          "uuid" : {
            "type" : "string"
          },
          "net_info" : {
            "$ref" : "#/components/schemas/net_info"
          }
        }
      },
      "net_info" : {
        "required" : [ "fund_code", "id", "net", "net_datetime" ],
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "integer",
            "format" : "uuid",
            "example" : 1
          },
          "fund_code" : {
            "type" : "string",
            "example" : "D100000001"
          },
          "net_datetime" : {
            "type" : "string",
            "format" : "YYYY-MM-DD",
            "example" : "2019-06-01"
          },
          "net" : {
            "type" : "integer",
            "format" : "int64",
            "example" : 1000000
          }
        }
      },
      "Benef_info" : {
        "required" : [ "birth", "risk", "risk_exp_date", "user_name", "uuid" ],
        "type" : "object",
        "properties" : {
          "benefit_id" : {
            "type" : "string"
          },
          "user_name" : {
            "type" : "string"
          },
          "birth" : {
            "type" : "string",
            "format" : "YYYY-MM-DD",
            "example" : "1991-12-31"
          },
          "risk" : {
            "type" : "integer",
            "format" : "int64",
            "example" : 10
          },
          "risk_exp_date" : {
            "type" : "string",
            "format" : "YYYY-MM-DD",
            "example" : "2019-01-01"
          }
        }
      },
      "BenefAccount" : {
        "required" : [ "account_records", "benefit_id" ],
        "type" : "object",
        "properties" : {
          "benefit_id" : {
            "type" : "string"
          },
          "account_records" : {
            "$ref" : "#/components/schemas/benef_account"
          }
        }
      },
      "benef_account" : {
        "required" : [ "account_category", "account_number", "bank_code", "capital" ],
        "type" : "object",
        "properties" : {
          "account_category" : {
            "type" : "integer",
            "format" : "int64",
            "example" : 0
          },
          "capital" : {
            "type" : "integer",
            "format" : "int64",
            "example" : 1000000
          },
          "bank_code" : {
            "type" : "string",
            "example" : "008"
          },
          "account_number" : {
            "type" : "string",
            "example" : "121200668858"
          },
          "create_date" : {
            "type" : "string",
            "format" : "YYYY-MM-DD",
            "example" : "2019-10-10"
          }
        }
      },
      "Inventory" : {
        "required" : [ "benefit_id", "inventory_records" ],
        "type" : "object",
        "properties" : {
          "benefit_id" : {
            "type" : "string"
          },
          "inventory_records" : {
            "$ref" : "#/components/schemas/inventory_records"
          }
        }
      },
      "inventory_records" : {
        "required" : [ "fund_code", "fund_name", "id", "inventory_cost", "inventory_unit", "price_currency" ],
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "integer",
            "format" : "uuid",
            "example" : 1
          },
          "fund_code" : {
            "type" : "string",
            "example" : "D100000001"
          },
          "fund_name" : {
            "type" : "string",
            "example" : "華南永昌永昌證券投資信託基金"
          },
          "price_currency" : {
            "type" : "string",
            "example" : "台幣(TWN)"
          },
          "inventory_unit" : {
            "type" : "integer",
            "format" : "int64",
            "example" : 10
          },
          "inventory_cost" : {
            "type" : "integer",
            "format" : "int64",
            "example" : 1000
          }
        }
      }
    },
    "securitySchemes" : {
      "ApiKeyAuth" : {
        "type" : "apiKey",
        "name" : "api_key",
        "in" : "query"
      }
    }
  }
}