        "parameters" : [ {
          "name" : "uuid",
          "in" : "query",
          "description" : "UUID",
          "required" : true,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "回傳基金淨值資料",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "array",
                  "items" : {
                    "$ref" : "#/components/schemas/Net"
                  }
                }
              }
            }
          }
        },
        "security" : [ {
          "ApiKeyAuth" : [ ]
        } ]
      }
    },
    "/beneficiary/information" : {
      "get" : {
        "tags" : [ "Beneficiary" ],
        "summary" : "查詢受益人基本資料",
        "description" : "## 利用 UUID 查詢受益人基本資料 \n---\n### 回傳參數說明\n| 參數名稱 | 中文名稱 |\n| ------- | -------- |\n| benefit_id   | 受益人編號    |\n| user_name   | 受益人姓名    |\n| birth   | 受益人出生年月日    |\n| risk   | 受益人之KYC風險承受度    |\n| risk_exp_date   | 受益人之KYC有效期限    |\n",
        "parameters" : [ {
          "name" : "uuid",
          "in" : "query",
          "description" : "UUID",
          "required" : true,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "回傳受益人基本資料",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Benef_info"
                }
              }
            }
          }
        },
        "security" : [ {
          "ApiKeyAuth" : [ ]
        } ]
      }
    },
    "/beneficiary/account" : {
      "get" : {
        "tags" : [ "Beneficiary" ],
        "summary" : "查詢受益人帳戶資料",
        "description" : "## 利用 UUID 查詢受益人帳戶資料 \n---\n### 回傳參數說明\n| 參數名稱 | 中文名稱 |\n| ------- | -------- |\n| benefit_id   | 受益人編號    |\n| account_category   | 帳號種類 (0:台幣, 1:指定外幣 2:綜合外幣)    |\n| capital   | 銀行資金    |\n| bank_code   | 銀行代碼    |\n| account_number   | 銀行帳號    |\n",
        "parameters" : [ {
          "name" : "uuid",
          "in" : "query",
          "description" : "UUID",
          "required" : true,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "回傳所有帳戶資料",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "array",
                  "items" : {
                    "$ref" : "#/components/schemas/BenefAccount"
                  }
                }
              }
            }
          }
        },
        "security" : [ {
          "ApiKeyAuth" : [ ]
        } ]
      }
    },
    "/beneficiary/inventory" : {
      "get" : {
        "tags" : [ "Beneficiary" ],
        "summary" : "查詢受益人基金庫存資料",
        "description" : "## 利用 UUID 查詢受益人所有基金庫存資料 \n---\n### 回傳參數說明\n| 參數名稱 | 中文名稱 |\n| ------- | -------- |\n| benefit_id   | 受益人編號    |\n| fund_code   | 基金代碼    |\n| fund_name   | 基金名稱    |\n| price_currency   | 計價幣別    |\n| inventory_unit   | 庫存單位數    |\n| inventory_cost   | 投資成本    |\n",
        "parameters" : [ {
          "name" : "uuid",
          "in" : "query",
          "description" : "UUID",
          "required" : true,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "string"
          }
